package slice

import (
	"testing"

	"github.com/stretchr/testify/assert"
)

var unique{{.DataTypeCap}}Tests = []struct {
	name     string
	input    []{{.DataType}}
	expected []{{.DataType}}
}{
	{
		name: "no duplicates",
		input: []{{.DataType}}{
			"test1",
			"test2",
		},
		expected: []{{.DataType}}{
			"test1",
			"test2",
		},
	},
	{
		name: "one duplicates",
		input: []{{.DataType}}{
			"test1",
			"test1",
			"test2",
		},
		expected: []{{.DataType}}{
			"test1",
			"test2",
		},
	},
	{
		name: "multiple duplicates",
		input: []{{.DataType}}{
			"test1",
			"test1",
			"test2",
			"test2",
			"test2",
			"test1",
			"test1",
			"test2",
			"test1",
		},
		expected: []{{.DataType}}{
			"test1",
			"test2",
		},
	},
}

func TestUnique{{.DataTypeCap}}(t *testing.T) {
	for _, tt := range unique{{.DataTypeCap}}Tests {
		t.Run(tt.name, func(t *testing.T) {
			output := Unique{{.DataTypeCap}}(tt.input)
			assert.Equal(t, output, tt.expected)
		})
	}
}
